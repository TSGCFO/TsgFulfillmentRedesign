{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "server:test": "NODE_ENV=test tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:dev": "NODE_ENV=development vite build",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "./scripts/start-dev.sh",
    "setup:dev": "./scripts/setup-dev-env.sh",
    "check": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:dev": "NODE_ENV=development drizzle-kit migrate",
    "db:migrate:test": "NODE_ENV=test drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:push:dev": "NODE_ENV=development drizzle-kit push",
    "db:push:test": "NODE_ENV=test drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed:dev": "NODE_ENV=development tsx scripts/seed-development.js",
    "test": "vitest run",
    "test:dev": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:api": "NODE_ENV=test vitest run server/__tests__/routes/",
    "test:integration": "NODE_ENV=test vitest run server/__tests__/integration.test.ts",
    "test:unit": "NODE_ENV=test vitest run --exclude='**/integration.test.ts' --exclude='e2e/**'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "NODE_ENV=test vitest run --coverage --reporter=json --outputFile=test-results/vitest-results.json",
    "e2e": "npx playwright install --with-deps && playwright test",
    "e2e:install": "npx playwright install --with-deps",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "logs:dev": "tail -f logs/development.log",
    "logs:backend": "tail -f logs/backend-dev.log",
    "logs:frontend": "tail -f logs/frontend-dev.log",
    "clean": "rm -rf dist/ build/ coverage/ test-results/ .vite/",
    "clean:dev": "rm -rf logs/ temp/ uploads/ .dev-status"
  },
